{
  "name": "proyecto4",
  "version": "1.0.0",
  "description": "API Express",
  "main": "server.ts",
  "scripts": {
    "test": "tsc && jest",
    "dev": "nodemon ./src/server.ts",
    "seeds": "ts-node ./src/database/seeders/seeders.ts",
    "run-migrations": "typeorm-ts-node-commonjs migration:run -d ./src/database/db.ts",
    "revert-migrations": "typeorm-ts-node-commonjs migration:revert -d ./src/database/db.ts",
    "build": "tsc",
    "start": "node ./dist/server.js",
    "testing": "tsc && cross-env DOTENV_CONFIG_PATH=.env.testing jest --setupFiles=dotenv/config"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "5.1.1",
    "cross-env": "^7.0.3",
    "dotenv": "16.4.5",
    "express": "4.18.2",
    "jsonwebtoken": "9.0.2",
    "mysql2": "3.9.2",
    "reflect-metadata": "0.2.1",
    "typeorm": "0.3.20"
  },
  "devDependencies": {
    "@types/bcrypt": "5.0.2",
    "@types/express": "4.17.21",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "9.0.6",
    "@types/node": "20.11.20",
    "@types/supertest": "^6.0.2",
    "jest": "^29.7.0",
    "nodemon": "3.1.0",
    "supertest": "^6.3.4",
    "ts-node": "10.9.2",
    "typescript": "5.3.3"
  },
  "jest": {
    "modulePathIgnorePatterns": [
      "src"
    ]
  }
}
